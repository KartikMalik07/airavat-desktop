{
  "name": "airavat-desktop",
  "version": "1.0.0",
  "description": "Offline desktop elephant-detection and identification tool",
  "main": "./src/main/main.js",
  "homepage": ".",
  "scripts": {
    "dev": "electron . --dev",
    "electron": "electron .",
    "setup": "node src/main/model-manager.js --download",
    "package:win": "electron-packager . Airavat --overwrite --platform=win32 --arch=x64 --icon=assets/icon.ico --asar --out=release-builds",
    "package:mac": "electron-packager . Airavat --overwrite --platform=darwin --arch=x64 --icon=assets/icon.icns --asar --out=release-builds",
    "package:linux": "electron-packager . Airavat --overwrite --platform=linux --arch=x64 --icon=assets/icon.png --asar --out=release-builds"
  },
  "keywords": [
    "elephant",
    "detection",
    "yolo",
    "siamese",
    "offline",
    "electron"
  ],
  "author": "Kartik Malik",
  "license": "MIT",
  "devDependencies": {
    "electron": "^37.2.6",
    "electron-packager": "^17.1.2",
    "electron-reload": "^2.0.0-alpha.1"
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "form-data": "^4.0.4",
    "fs-extra": "^11.3.1",
    "glob": "^10.4.5"
  }
}
